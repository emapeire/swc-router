{
  "name": "swc-router",
  "description": "A react router with swc",
  "version": "0.0.17",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "npm run echo && swc src/components src/utils src/index.jsx -d dist",
    "echo": "echo",
    "test": "vitest --watch",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "release": "release-it"
  },
  "dependencies": {
    "path-to-regexp": "8.0.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.99",
    "@testing-library/react": "14.1.2",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@vitejs/plugin-react-swc": "3.11.0",
    "eslint": "8.53.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.4",
    "happy-dom": "15.10.2",
    "release-it": "^19.0.4",
    "vite": "7.1.5",
    "vitest": "3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emapeire/swc-router.git"
  },
  "homepage": "https://github.com/emapeire/swc-router#readme",
  "bugs": {
    "url": "https://github.com/emapeire/swc-router/issues"
  },
  "keywords": [
    "react",
    "router",
    "swc"
  ],
  "author": "Emanuel Peire",
  "license": "MIT"
}
